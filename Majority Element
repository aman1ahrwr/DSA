int majorityElement(int a[], int size)
    {
        if(size==1)
            return a[0];
       sort(a, a+size);
       int ct=1;
       for(int i=0;i<size-1;i++)
       {
           if(a[i]==a[i+1])
                ct++;
            else 
                ct=1;
                
            if(ct>size/2)
                return a[i];
       }
        
        return -1;
    }
